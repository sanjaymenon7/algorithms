package Set3Q4SuperMarket;

public class Vertex {
	
	 public int node;
	    public int distance;

	    public Node(int node, int distance) {
	        this.node = node;
	        this.distance = distance;
	    }

	    @Override
	    public int compareTo(Node o) {
	        return Integer.compare(distance, o.distance);
	    }

}
